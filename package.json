{
  "name": "s3-browser",
  "version": "1.2.5",
  "description": "Multi-platform S3 Browser Desktop Application",
  "author": {
    "name": "Growly",
    "email": "open@getgrowly.co"
  },
  "private": true,
  "main": "electron/dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/getgrowly/s3-browser.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --cache --cache-location .eslintcache",
    "lint:fix": "eslint . --fix --cache --cache-location .eslintcache",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "check:types": "tsc --noEmit && tsc --noEmit -p electron/tsconfig.json",
    "check:lint": "eslint . --cache --cache-location .eslintcache",
    "check:format": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "check:all": "npm run check:types && npm run check:lint && npm run check:format",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "export": "next build",
    "generate-icons": "node scripts/generate-icons.js",
    "electron:dev": "concurrently \"next dev -p 54000\" \"wait-on tcp:54000 && npx tsc -p electron/tsconfig.json && electron .\"",
    "electron:build": "next build && npx tsc -p electron/tsconfig.json && electron-builder",
    "electron:build:mac": "electron-builder --mac --publish always",
    "electron:build:win": "next build && npx tsc -p electron/tsconfig.json && electron-builder --win",
    "electron:build:linux": "next build && npx tsc -p electron/tsconfig.json && electron-builder --linux",
    "electron:build:all": "next build && npx tsc -p electron/tsconfig.json && electron-builder --mac --win --linux",
    "commit": "cz",
    "prepare": "husky",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@hookform/resolvers": "^3.9.1",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@mui/material-pigment-css": "latest",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-virtual": "^3.13.12",
    "@vercel/analytics": "1.3.1",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "4.1.0",
    "electron-log": "^5.2.2",
    "electron-updater": "^6.3.9",
    "geist": "^1.3.1",
    "lucide-react": "^0.454.0",
    "next": "^15.0.0",
    "next-themes": "latest",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7.54.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.24.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.38.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "commitizen": "^4.3.1",
    "concurrently": "^9.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^35.7.5",
    "electron-builder": "^25.1.8",
    "eslint": "^9",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.3.0",
    "png2icons": "^2.0.1",
    "prettier": "^3.4.2",
    "semantic-release": "^24.0.0",
    "typescript": "^5",
    "typescript-eslint": "^8.18.2",
    "vitest": "^2.1.8",
    "wait-on": "^8.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "build": {
    "appId": "com.growly.s3browser",
    "productName": "Growly S3",
    "publish": [
      {
        "provider": "github",
        "owner": "getgrowly",
        "repo": "s3-browser",
        "releaseType": "release",
        "publishAutoUpdate": true
      }
    ],
    "files": [
      ".next/standalone/**/*",
      ".next/static/**/*",
      "public/**/*",
      "!public/**/*.md",
      "electron/dist/**/*",
      "node_modules/better-sqlite3/**/*",
      "package.json"
    ],
    "asarUnpack": [
      "node_modules/better-sqlite3/**/*"
    ],
    "directories": {
      "buildResources": "public",
      "output": "release"
    },
    "extraResources": [
      {
        "from": "public",
        "to": "public",
        "filter": [
          "**/*"
        ]
      }
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "public/icon.icns",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "identity": null,
      "extendInfo": {
        "CFBundleName": "Growly S3",
        "CFBundleDisplayName": "Growly S3"
      }
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "msi",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "public/icon.ico",
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "category": "Development",
      "icon": "public/s3-logo.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Growly S3"
    }
  }
}
